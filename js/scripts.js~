var testData = {
    api:    'inventory',
    method: 'getLatestScan'
};

sg.makePostCall("api/index.php", testData)
    .success(function(data){
        console.log('done?');
        console.log(data);
    })
    .fail(function(sender, message, details){
        alert("Sorry, something went wrong!");
    });
